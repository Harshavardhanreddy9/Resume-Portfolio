{
  "name": "interactive-3d-resume",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://harshavardhanreddy9.github.io/Resume-Portfolio",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview --port 5173",
    "start:check": "npm run dev & ./scripts/health-check.sh",
    "deploy:vercel": "vercel --prod",
    "deploy:gh-pages": "npm run build && gh-pages -d dist",
    "health-check": "node health-check.js",
    "dev:check": "npm run dev & wait-on http://localhost:5173 && npm run health-check",
    "type-check": "tsc --noEmit",
    "build:check": "npm run build && npm run preview"
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@react-three/drei": "^9.88.13",
    "@react-three/fiber": "^8.15.11",
    "framer-motion": "^10.16.16",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "three": "^0.158.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "@types/three": "^0.158.3",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "gh-pages": "^6.1.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "wait-on": "^7.2.0"
  }
}
